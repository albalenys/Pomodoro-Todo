<div class="center register-user">
  <h3>Log In</h3>
  <%= form_for @user, html: {class: 'fancybox-form'}, url: {action: "auth"} do |f| %>
    <%= f.text_field :email, placeholder: "email" %><br>
    <%= f.password_field :password, placeholder: "password" %><br>
    <%= f.submit "Submit", class: "btn btn-secondary" %>
  <% end %>
</div>